	<xs:simpleType name="exclusivePeriodType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="0" />
			<xs:enumeration value="2" />
			<xs:enumeration value="4" />
			<xs:enumeration value="8" />
			<xs:enumeration value="lifetime" />
		</xs:restriction>
	</xs:simpleType>
